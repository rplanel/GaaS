  "scripts": {
    "test": "pnpm --filter './packages/**' test",
    "dev": "pnpm --filter ./playground dev",
    "lint": "eslint .",
    "cleandep": "rm -rf node_modules && rm -rf pnpm-lock.yaml && pnpm --filter '**' run cleandep",
