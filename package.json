    "eslint-plugin-format": "catalog:",
    "fs-extra": "catalog:",
    "jiti": "catalog:",
    "lint-staged": "catalog:",
    "simple-git-hooks": "catalog:"
  },
  "simple-git-hooks": {
    "pre-commit": "pnpm lint-staged"
  },
  "lint-staged": {
    "*": "eslint --fix"
  }
